<Table>
    <Name>DYN_TP</Name>
    <Description>Reporting Data Model: Audit transaction parameter table (deal expired between date 1 &amp; 2)</Description>
    <Type>Transaction</Type>
    <Directory>Murex</Directory>
    <Class>DYN_TRNRP_PL</Class>
    <History>TP</History>
    <Alias>TRNRP_PL.DBF</Alias>
    <LoadByPosition>No</LoadByPosition>
    <Fields>
        <Field Type="Horizontal">
            <Label>ACC_CUR</Label>
            <Description>Accounting currency</Description>
            <Type>Character</Type>
            <Length>3</Length>
            <Decimal>0</Decimal>
            <FieldType>Default</FieldType>
            <Formula Nb="0">CENV('CUR_ACC')</Formula>
        </Field>
        <Field Type="Horizontal">
            <Label>G_FGT_OPT</Label>
            <Description>Family Group Type Category - Option indicator</Description>
            <Type>Character</Type>
            <Length>3</Length>
            <Decimal>0</Decimal>
            <FieldType>Default</FieldType>
            <Formula Nb="0">IIF(TRN_GRP=="OPT".OR.TRN_GRP="OSWP".OR.TRN_GRP="CF".OR.TRN_GRP="IRG","OPT","UND")</Formula>
        </Field>
        <Field Type="Horizontal">
            <Label>G_FGT_SEC</Label>
            <Description>Family Group Type Category - Security Focus</Description>
            <Type>Character</Type>
            <Length>30</Length>
            <Decimal>0</Decimal>
            <FieldType>Default</FieldType>
            <Formula Nb="0">IIF(TRN_TYPE="LST".OR.TRN_TYPE="ORG".OR."FUT"$TRN_GRP,"Listed Products",IIF(TRN_GRP="EQUIT".OR.TRN_GRP="WARNT".OR.TRN_GRP="BOND","Securities",IIF(TRN_GRP="OPT","OTC/Exotic Opt",IIF(TRN_GRP="FXD","Foreign Exchange",TP_DSCTYPE))))</Formula>
        </Field>
        <Field Type="Horizontal">
            <Label>G_FGT_SWLK</Label>
            <Description>Family Group Type Category - Swap like indicator: true if generating period schedules</Description>
            <Type>Number</Type>
            <Length>1</Length>
            <Decimal>0</Decimal>
            <FieldType>Default</FieldType>
            <Formula Nb="0">IIF(TRN_GRP="CD".OR.TRN_GRP=="LN_BR".OR.TRN_GRP=="FRA".OR.(" swaps"$LOWER(TP_DSCTYPE).AND.TRN_TYPE&lt;&gt;"XSW").OR.TRN_GRP=="IRG".OR.TRN_GRP=="CF".OR."repo"$LOWER(TP_DSCTYPE),1,0)</Formula>
        </Field>
        <Field Type="Horizontal">
            <Label>G_F_STL</Label>
            <Description>Flow Settlement Party</Description>
            <Type>Character</Type>
            <Length>10</Length>
            <Decimal>0</Decimal>
            <FieldType>Default</FieldType>
            <Formula Nb="0">IIF(G_FGT_SEC="Listed",TP_CNTRP,IIF(G_FGT_SEC="OPT".OR.G_FGT_SEC="Secu",TP_SECSTL,""))</Formula>
        </Field>
        <Field Type="Horizontal">
            <Label>G_INS_FXBU</Label>
            <Description>Instrument - Base-Underlying ccies for warrant and FX trades</Description>
            <Type>Character</Type>
            <Length>20</Length>
            <Decimal>0</Decimal>
            <FieldType>Default</FieldType>
            <Formula Nb="0">IIF(TRN_GRP=="WARNT",RSKSECTION,IIF(TRN_FMLY=="CURR",TP_FXBASE-"-"-TP_FXUND,INSTRUMENT))</Formula>
        </Field>
        <Field Type="Horizontal">
            <Label>G_INS_FXRK</Label>
            <Description>Instrument -  Risk section for FX trades and warrants</Description>
            <Type>Character</Type>
            <Length>20</Length>
            <Decimal>0</Decimal>
            <FieldType>Default</FieldType>
            <Formula Nb="0">IIF(TRN_GRP=='WARNT'.OR.TRN_FMLY=='CURR',RSKSECTION,INSTRUMENT)</Formula>
        </Field>
        <Field Type="Horizontal">
            <Label>G_INS_WRK</Label>
            <Description>Instrument - Risk section for warrant</Description>
            <Type>Character</Type>
            <Length>20</Length>
            <Decimal>0</Decimal>
            <FieldType>Default</FieldType>
            <Formula Nb="0">IIF(TRN_GRP=='WARNT',RSKSECTION,INSTRUMENT)</Formula>
        </Field>
        <Field Type="Horizontal">
            <Label>G_LIVE2</Label>
