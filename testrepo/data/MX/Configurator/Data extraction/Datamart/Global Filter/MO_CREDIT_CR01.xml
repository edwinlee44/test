<filterTemplate>
    <Label>MO_CREDIT_CR01</Label>
    <Description>Only Feeding Live Deals</Description>
    <Owner>ITSO001</Owner>
    <Group>IT_CONF</Group>
    <Visibility>E</Visibility>
    <UpdateRight>O</UpdateRight>
    <CreationClass>DYN_TRNRP_PL</CreationClass>
    <ClassType>transaction</ClassType>
    <ClassSubType>-1</ClassSubType>
    <Options>
        <Dates>
            <Date Nb="0">
                <Type>No selection</Type>
                <Description/>
                <MarketDataSet/>
                <UseEntityClosingSet>No</UseEntityClosingSet>
                <MDCSSetting>default</MDCSSetting>
                <MDSType>default</MDSType>
            </Date>
            <Date Nb="1">
                <Type>No selection</Type>
                <Description/>
                <MarketDataSet/>
                <UseEntityClosingSet>No</UseEntityClosingSet>
                <MDCSSetting>default</MDCSSetting>
                <MDSType>default</MDSType>
            </Date>
            <Date Nb="2">
                <Type>Reporting date</Type>
                <Description/>
                <DateOutput>20201127</DateOutput>
                <MarketDataSet/>
                <UseEntityClosingSet>No</UseEntityClosingSet>
                <MDCSSetting>default</MDCSSetting>
                <MDSType>default</MDSType>
                <TimeStamp>
                    <Type>4</Type>
                    <TimeStampDate>
                        <Type>No selection</Type>
                        <Offset>0</Offset>
                    </TimeStampDate>
                    <TimeStampTime>
                        <Type>No selection</Type>
                        <Offset>0</Offset>
                    </TimeStampTime>
                </TimeStamp>
            </Date>
        </Dates>
        <Filter>
            <Classification>
                <businessObjectId mefClass="mxSystemClassificationIREQUEST">
                    <primarySystem>MX</primarySystem>
                </businessObjectId>
                <objectClassifications/>
            </Classification>
            <Typology>
                <businessObjectId mefClass="mxSystemClassificationIREQUEST">
                    <primarySystem>MX</primarySystem>
                </businessObjectId>
                <objectClassifications/>
            </Typology>
            <Tags/>
            <EventTypes/>
            <FilterExpression Type="ExcludePLAdjustment">TRN_STATUS&lt;&gt;'DEAD'.OR.RQWHERE("TRN_HDR_DBF.M_TRN_GTYPE=92 and TRN_HDR_DBF.M_CONTRACT in (select M_CONTRACT from TRN_HDR_DBF where M_TRN_GTYPE=92 and M_TRN_STATUS&lt;&gt;'DEAD')", "")</FilterExpression>
            <FilterExpression Type="Restricted">TRN_FMLY=="CRD" .AND. (TRN_GRP == "CDS" .OR. TRN_GRP == "OBDS")</FilterExpression>
            <Benchmarks/>
            <WorkflowAudit/>
        </Filter>
        <BuiltOn>Detailed base</BuiltOn>
        <LateTrading>Default</LateTrading>
        <OverrideDynTablesFilter>No</OverrideDynTablesFilter>
        <StpRightDataRef>0</StpRightDataRef>
    </Options>
</filterTemplate>
