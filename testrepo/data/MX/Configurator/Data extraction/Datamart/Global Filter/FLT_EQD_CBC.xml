    <CreationClass>DYN_TRNRP_PL</CreationClass>
    <ClassType>transaction</ClassType>
    <ClassSubType>-1</ClassSubType>
    <Options>
        <Dates>
            <Date Nb="2">
                <Type>Contextual today</Type>
                <Description/>
                <DateOutput>20201130</DateOutput>
                <MarketDataSet/>
