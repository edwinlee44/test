            <Typology>
                <businessObjectId mefClass="mxSystemClassificationIREQUEST">
                    <primarySystem>MX</primarySystem>
                </businessObjectId>
                <objectClassifications/>
            </Typology>
            <Tags/>
            <EventTypes/>
            <FilterExpression Type="ExcludePLAdjustment">(BINTERNAL&lt;&gt;SINTERNAL.OR.((BINTERNAL==SINTERNAL).AND.(TRN_FMLY=='IRD').AND.(TRN_GRP=='ASWP'))).AND.((TRN_FMLY=='EQD'.AND.(TRN_GRP=='BOND'.OR.TRN_GRP=='FUT'.OR.TRN_GRP=='OPT')).OR.(TRN_FMLY=='SCF'.AND.TRN_TYPO=='STK OPT EX').OR.(TRN_FMLY=='CRD'.AND.TRN_GRP=='OBDS').OR.(TRN_FMLY=='IRD'.AND.TRN_GRP=='ASWP'))</FilterExpression>
            <Benchmarks/>
            <WorkflowAudit/>
        </Filter>
        <BuiltOn>Detailed base</BuiltOn>
        <LateTrading>Default</LateTrading>
        <OverrideDynTablesFilter>No</OverrideDynTablesFilter>
        <StpRightDataRef>0</StpRightDataRef>
